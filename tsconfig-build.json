{
    "compilerOptions": {
        "declaration": true,
        "strict": true,
        "strictPropertyInitialization": false,
        "noFallthroughCasesInSwitch": true,
        "moduleResolution": "node",
        "module": "es2015",
        "target": "es2015",
        "lib": [
            "es2015",
            "dom"
        ],
        "skipLibCheck": true,
        "types": [],
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "sourceMap": true,
        "inlineSources": true,
        "importHelpers": false
    },
    "bazelOptions": {
        "suppressTsconfigOverrideWarnings": true
    }
}